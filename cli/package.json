    "babel-preset-stage-0": "6.24.1",
    "blueimp-md5": "2.10.0",
    "body-parser": "1.18.3",
    "case-sensitive-paths-webpack-plugin": "2.1.2",
    "cli-clear": "1.0.4",
    "cli-color": "1.2.0",
    "commander": "2.9.0",
    "cors": "2.8.4",
    "css-loader": "1.0.0",
    "dotenv": "4.0.0",
    "dotenv-webpack": "1.5.7",
    "express": "4.16.3",
    "file-loader": "1.1.11",
    "filendir": "1.0.0",
    "findup": "0.1.5",
    "firebase": "4.0.0",
    "fs-readdir-recursive": "1.1.0",
    "interpret": "1.1.0",
    "lodash": "4.17.21",
